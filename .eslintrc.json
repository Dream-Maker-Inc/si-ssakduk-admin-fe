{
  "extends": "next/core-web-vitals",
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": "latest",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "react/no-unknown-property": [
      "error",
      {
        "ignore": ["css"]
      }
    ]
  }
}
